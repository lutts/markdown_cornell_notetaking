{
	"scopeName": "cornell-question.injection",
	"injectionSelector": "L:text.html.markdown",
	"patterns": [
	  { "include": "#cornell-question" }
	],
	"repository": {
	  "cornell-question": {
		"patterns": [
			{
				"match": "^(q|Q):.*$",
				"name": "cornell.question"
			},
			{
				"match": "^(a|A): ",
				"name": "cornell.answer"
			},
			{
				"match": "{{(.*?)}}((?:\\([^)]*?\\))?)",
				"name": "supermemo.cloze"
			}
		]
	  }
	}
  }
  